{
    "name": "blinq/synth",
    "description": "This is my package synth",
    "keywords": [
        "blinq",
        "laravel",
        "synth"
    ],
    "homepage": "https://github.com/blinq/synth",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennard",
            "email": "lennard@blinq.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pcntl": "*",
        "blinq/openai": "^1.0.3",
        "openai-php/client": "^0.6.2",
        "scrivo/highlight.php": "v9.18.1.10",
        "spatie/ignition": "^1.9",
        "spatie/laravel-package-tools": "^1.14.0",
        "symfony/http-client": "^6.3"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Blinq\\Synth\\": "src",
            "Blinq\\Synth\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blinq\\Synth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blinq\\Synth\\SynthServiceProvider"
            ],
            "aliases": {
                "Synth": "Blinq\\Synth\\Facades\\Synth"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
